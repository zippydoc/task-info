{
  "description": "Compare country COVID-19 in a D3 Sunburst Chart; datasource: API of datacommons.org",
  "image": "https://raw.githubusercontent.com/zippydoc/Pictograms/master/Covid-19_Snbst.gif",
  "scriptSettings": {
    "autoLoadTable": [
    ],
    "countries": [
      "US",
      "UK"
    ],
    "languages": [
      "EN"
    ],
    "displayName": "COVID-19 (datacom.)",
    "payingUsersOnly": false,
    "explanationPicture": "",
    "type": "Facts & Figures",
	"attributes": [
      "0-input","Guest-User","Starter-Screen"
	]
  },
  "menu": [
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/DCCOVID19WorldDataOptions.json?ref=prod",
      "children": [
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/YearsWithoutBreakline.json?ref=prod",
          "property": "options_frame/fields",
          "after": "options.per_capita"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/Months.json?ref=prod",
          "property": "options_frame/fields",
          "after": "options.sel_year"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/Countries.json?ref=prod",
          "property": "options_frame/fields",
          "after": "options.all_countries"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/DataCommonsDescription.json?ref=prod",
          "property": "options_frame/fields"
        }
      ]
    },
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/DataCommonsOutput.json?ref=prod",
      "children": [
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/OutputConfig.json?ref=prod",
          "property": "output/fields",
          "after": "output.custom_options"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/DownloadSettings.json?ref=prod",
          "property": "output/fields",
          "after": "output_config_frame"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/IfTableExist.json?ref=prod",
          "property": "output/fields/output_config_frame/fields",
          "after": "output.tag_name"
        }
      ]
    }
  ],
  "script": "https://api.github.com/repos/zippydoc/basic/contents/DataCommonsWorldData.py?ref=prod",
  "version": 1.0
}
