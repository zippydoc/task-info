{
  "description": "Create PDF in your PDFrePRO account PDF templates and export to cloud directory",
  "image": "https://raw.githubusercontent.com/zippydoc/Pictograms/master/PDF-Writer.gif",
  "scriptSettings": {
    "autoLoadTable": [
    ],
    "countries": [
      "US",
      "UK"
    ],
    "languages": [
      "EN"
    ],
    "displayName": "Create Custom PDF",
    "payingUsersOnly": false,
    "explanationPicture": "",
    "type": "Automate",
	"attributes": [
      "0-input","Guest-User","Starter-Screen","MS-User-Only"
	],
    "ms_oauth2_scope": "files.readwrite.all",
    "ms_oauth2_menu_condition": "options.type=='create' && options.webservice=='ms_one_drive' && (options.where_to_create=='only_export_to_cloud' || options.where_to_create=='both_export_to_cloud_and_open_in_browser')"
  },
  "menu": [
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/PdfReProCustomOptions.json?ref=prod"
    },
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/PdfOutput.json?ref=prod",
      "children": [
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/AddTagWithoutInputTable.json?ref=prod",
          "property": "output/fields",
          "after": "output.custom_options"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/DownloadSettings.json?ref=prod",
          "property": "output/fields",
          "after": "output_config_frame"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/IfTableExist.json?ref=prod",
          "property": "output/fields/output_config_frame/fields",
          "after": "output.tag_name"
        }
      ]
    }
  ],
  "script": "https://api.github.com/repos/zippydoc/basic/contents/PdfRePro.py?ref=prod",
  "version": 1.0
}
